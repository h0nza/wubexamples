# Add a listener on port 8080
Listener {
    -port 8080
}

# Add a Direct domain under /object
/object/ {
    domain Direct
    object $oodomain
}

# Include jQuery and Icons
/jquery/ {
    domain JQ
}

/icons/ {
    domain Icons
}

# CSS File domain used in some tests
/css/ {
    domain File
    root docroot/css
}
